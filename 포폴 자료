    sidebarLinks.forEach(link => {
        link.addEventListener('click', (e) => {
            e.preventDefault(); // ЖИ░в│И aьЃюЖиИ ВЮ┤вЈЎ вДЅЖИ░
            const day = link.getAttribute('data-day');
            main.innerHTML = dayContents[day] || `<h1>${day}</h1><p>ВЋёВДЂ Вцђв╣ё ВцЉВЮИ вѓ┤ВџЕВЮ┤ВЌљВџћ~ ­ЪЏа№ИЈ</p>`;
        });
    });

    // РюЁ Home в▓ёьі╝ ьЂ┤вдГ ВІю В┤ѕЖИ░ вѓ┤ВџЕВю╝вАю вљўвЈївдгЖИ░!
    homeLink.addEventListener('click', (e) => {
        e.preventDefault();
        main.innerHTML = initialMainContent;
    });

    function openPopup() {
        const result = confirm("­ЪїИ ВЮ┤ HTML ьјўВЮ┤ВДђвіћ ­ЪїИ\n\n2025вЁё 4ВЏћ 22ВЮ╝ вХђьё░ 5ВЏћ 29ВЮ╝Ж╣їВДђВЮў \nHTML ВѕўВЌЁ вѓ┤ВџЕВЮё ВаЋвдгьЋю вїђВІюв│┤вЊюВъёвІц!\n\nВЎ╝Вфй ВѓгВЮ┤вЊюв░ћВЌљВёю Ж░Ђ ьџїВ░евАю ВЮ┤вЈЎ Ж░ђвіЦьЋевІц! ­ЪЦ░­ЪЊџ\n\nВЮ┤ вѓ┤ВџЕВЮё Ж│ёВєЇ в│┤ВІювацвЕ┤ ьЎЋВЮИ !");

        if (result) {
            alert("ВбІВЋёВџћ! ­ЪцЊ ьѓх Ж│аВъЅ~! ­ЪїЪРюе");
        } else {
            alert("ьЎЋВЮИ ВЋѕ вѕївЪгвЈё ВЃЂЖ┤ђ ВЌєВДђвА▒ ( Рђб╠ђ ¤Ѕ Рђб╠Ђ )Рюе");
        }
    }
